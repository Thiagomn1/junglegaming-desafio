{
  "name": "jungle-challege",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "packageManager": "npm@10.9.2",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "turbo run dev --parallel",
    "dev:services": "turbo run dev --parallel --filter='./apps/*'",
    "dev:infra": "docker-compose -f docker-compose.dev.yml up -d",
    "dev:infra:stop": "docker-compose -f docker-compose.dev.yml down",
    "build": "turbo run build",
    "build:packages": "turbo run build --filter='./packages/*'",
    "start": "turbo run start",
    "start:dev": "turbo run start:dev --parallel",
    "lint": "turbo run lint",
    "lint:fix": "turbo run lint -- --fix",
    "test": "turbo run test",
    "clean": "turbo run clean && rm -rf node_modules",
    "setup": "npm install && npm run build:packages"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "turbo": "^2.6.0"
  }
}
